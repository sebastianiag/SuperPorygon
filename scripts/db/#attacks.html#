htsc('<a href="'+b+'"><img src="'+a+'" alt="'+c+'"></a>')+'"><br><label class="small">BBcode (forums and forum signatures):</label><br><input type="text" readonly class="input-text" value="'+htsc(g)+'"><br> <label class="small">Markdown (for Pok\u00e9Base):</label><br><input type="text" readonly class="input-text" value="'+htsc(d)+'">'};
$(function(){$(".main-menu-list > li > a").removeAttr("href");$(".main-menu-list").superfish({delay:800,speed:300,speedOut:300,cssArrows:!1});var a={tmplResult:function(a){var b="egg_group"==a.type?"mechanics/egg-groups":a.type,d=a.type.charAt(0).toUpperCase()+a.type.substr(1).replace("_"," ");return'<div class="sitesearch-results-item"><a class="ent-name" href="/'+b+"/"+a.alias+'">'+a.name+'</a> <small class="aside">('+d+")</small></div>"},outputResults:function(b){if(0===b.length)$("#sitesearch-results").slideUp();
else{entHtml="";for(var g in b)entHtml+=a.tmplResult(b[g]);$("#sitesearch-results").html(entHtml).slideDown()}},appMain:function(){$("#sitesearch").on("keyup focus",function(){var c=$(this).val();if(1<c.length){var g=b.table("searchdata"),d=[],e;for(e in g){for(var m in g[e]){var f=g[e][m];if(f[0].contains(c)||f[1].contains(c))if(d.push({type:e,name:g[e][m][0],alias:g[e][m][1]}),12<=d.length)break}if(12<=d.length)break}a.outputResults(d)}else a.outputResults([])}).on("blur",function(){var a=$(this);
setTimeout(function(){a.siblings("#sitesearch-results").hide()},200)})}},b=$.localDb("/json/",12);$("#sitesearch").on("focus",function(){b.load(["searchdata"],a.appMain)});$.fn.disableBlock=function(a){return"on"==a?this.each(function(){$(this).prevAll(".table-disabled").css({visibility:"visible"})}):"off"==a?this.each(function(){$(this).prevAll(".table-disabled").css({visibility:"hidden"})}):this.each(function(){$(this).wrap('<div class="table-wrap" />').before('<div class="table-disabled" />')})}});
$(window).load(function(){$(".pki, .pkg").each(function(){var a=$(this);a.addClass(a.data("sprite"))})});
