<script>

$(function () {
	var $table = $('.data-table');
	// set up table sorting
	$table.stupidtable(stblDataTypes);
	$('thead th:first-child', $table).click();
	// table filtering
	$table.filtable({ controlPanel: $('.table-filters') });
});

