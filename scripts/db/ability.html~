				<option value="">- All -</option>
							<option value="3">Generation 3</option>
							<option value="4">Generation 4</option>
							<option value="5">Generation 5</option>
							<option value="6">Generation 6</option>
						</select>
		</div>

		<table id="abilities" class="data-table wide-table">
		<thead>
			<tr>
				<th class="sorting" data-sort="string"><div class="sortwrap">Name</div></th> <th class="sorting" data-sort="int"><div class="sortwrap">Pokémon</div></th> <th><div class="sortwrap">Description</div></th> <th class="sorting" data-sort="int"><div class="sortwrap">Gen.</div></th> 			</tr>
		</thead>
		<tbody>
